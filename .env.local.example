# AnythingLLM Configuration
ANYTHINGLLM_ENDPOINT=http://localhost:3001
ANYTHINGLLM_KEY=your-api-key-here

# Mock Data (set to 'true' to use mock data instead of real API)
USE_MOCK_DATA=false

# OpenAI Configuration (used for Financial analysis, 5.* models are not supported yet)
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4.1-mini

# Logging Configuration
# Options: debug, info, warn, error, none
# - debug: Show all logs including debug messages
# - info: Show info, warn, and error messages
# - warn: Show only warnings and errors
# - error: Show only errors
# - none: Disable all logging
NEXT_PUBLIC_LOG_LEVEL=info
